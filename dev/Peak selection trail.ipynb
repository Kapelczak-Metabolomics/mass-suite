    "def mz_gen(mzml_scans, err_ppm = 20, mz_c_thres = 5):\n",
    "    pmz = []\n",
    "    for scan in mzml_scans:\n",
    "        pmz.append(scan.mz)\n",
    "    pmz = np.hstack(pmz).squeeze()\n",
    "    \n",
    "    def mz_list_gen(minmz, maxmz, error_ppm):\n",
    "        error = error_ppm * 1e-6\n",
    "        mz_list = [minmz]\n",
    "        mz = minmz\n",
    "        while mz <= maxmz:\n",
    "            mz = mz + 2 * error * mz\n",
    "            mz_list.append(mz)\n",
    "        return mz_list\n",
    "    mz_list = mz_list_gen(pmz.min(), pmz.max(), err_ppm)\n",
    "    \n",
    "    final_mz = []\n",
    "    for m in mz_list:\n",
    "        lm = m - err_ppm * 1e-6 * m\n",
    "        hm = m + err_ppm * 1e-6 * m\n",
    "        if len(pmz[(pmz <= hm) & (pmz >= lm)]) >= mz_c_thres:\n",
    "            final_mz.append(m)\n",
    "\n",
    "    return final_mz"
   ]
  },
